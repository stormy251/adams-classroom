{
  "name": "adams-classroom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --loglevel silent --write src && next lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run lint"
    }
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true
        }
      ]
    ]
  },
  "dependencies": {
    "framer-motion": "4.1.17",
    "styled-components": "5.2.1",
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.18",
    "@types/styled-components": "5.1.7",
    "babel-plugin-styled-components": "1.12.0",
    "eslint": "7.32.0",
    "husky": "4.3.8",
    "eslint-config-next": "11.1.0",
    "typescript": "4.3.5",
    "prettier": "2.2.1"
  }
}
